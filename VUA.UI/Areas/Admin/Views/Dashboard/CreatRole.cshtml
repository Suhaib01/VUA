@model VUA.Core.Models.ViewModels.CreatRoleViewModel
@{
    ViewData["Title"] = "CreatRole";
}

<h1>CreatRole</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreatRole" method="post">
            <div asp-validation-summary="All" class="text-danger" ></div>
            <div class="form-group">
                <label asp-for="RoleName" class="control-label">Role Name</label>
                <input asp-for="RoleName" class="form-control" />
                <span asp-validation-for="RoleName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Creat" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

